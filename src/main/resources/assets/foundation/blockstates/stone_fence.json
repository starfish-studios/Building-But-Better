{
  "multipart": [
    {
      "apply": {
        "model": "foundation:block/fence/stone_fence_post"
      },
      "when": {
        "pillar": "true"
      }
    },
    {
      "apply": {
        "model": "foundation:block/fence/stone_fence_side"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "foundation:block/fence/stone_fence_side",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "foundation:block/fence/stone_fence_side",
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "foundation:block/fence/stone_fence_side",
        "y": 270
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "foundation:block/fence/stone_fence_side_fill"
      },
      "when": {
        "north": "true",
        "side_fill": "true"
      }
    },
    {
      "apply": {
        "model": "foundation:block/fence/stone_fence_side_fill",
        "y": 90
      },
      "when": {
        "east": "true",
        "side_fill": "true"
      }
    },
    {
      "apply": {
        "model": "foundation:block/fence/stone_fence_side_fill",
        "y": 180
      },
      "when": {
        "south": "true",
        "side_fill": "true"
      }
    },
    {
      "apply": {
        "model": "foundation:block/fence/stone_fence_side_fill",
        "y": 270
      },
      "when": {
        "west": "true",
        "side_fill": "true"
      }
    }
  ]
}